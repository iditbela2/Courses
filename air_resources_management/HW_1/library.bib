file = {:Users/iditbela/Library/Application Support/Mendeley Desktop/Downloaded/Johnson et al. - 1998 - The Storm Cell Identification and Tracking Algorithm An Enhanced WSR-88D Algorithm.pdf:pdf},
isbn = {0882-8156},
issn = {0882-8156},
journal = {Weather and Forecasting},
