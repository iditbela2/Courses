journal = {Journal of Climatology},
keywords = {intensity orographic effect on,rainfall arid zone,rainfall intensity regional and,seasonal variations of rainfall},
number = {3},
pages = {277--291},
title = {{The distribution of rainfall intensity in Israel, its regional and seasonal variations and its climatological evaluation}},
url = {http://onlinelibrary.wiley.com/doi/10.1002/joc.3370060304/abstract{\%}5Cnhttp://onlinelibrary.wiley.com/store/10.1002/joc.3370060304/asset/3370060304{\_}ftp.pdf?v=1{\&}t=ht2qz53h{\&}s=03b46eff538abda2dd773acad36855699304ed0c},
