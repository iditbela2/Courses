doi = {10.1175/2010JHM1236.1},
file = {:Users/iditbela/Library/Application Support/Mendeley Desktop/Downloaded/Smith et al. - 2010 - The Hydrology and Hydrometeorology of Flooding in the Delaware River Basin.pdf:pdf},
isbn = {1525-755X$\backslash$r1525-7541},
issn = {1525-755X},
journal = {Journal of Hydrometeorology},
