number = {3},
pages = {789--800},
title = {{Rainfall thresholds for flood triggering. The case of Marathonas in Greece}},
volume = {60},
year = {2012}
}
@article{Mei2014,
abstract = {AbstractAccurate quantitative precipitation estimation (QPE) over mountainous basins is of great importance because of their susceptibility to hazards such as flash floods, shallow landslides and debris flow, triggered by heavy precipitation events (HPEs). In situ observations over mountainous areas are limited, but currently available satellite precipitation products can potentially provide the precipitation estimation needed for hydrological applications. In this study, four widely used satellite-based precipitation products (3B42-V7, 3B42-RT, CMORPH and PERSIANN) are evaluated with respect to their performance in capturing the properties of HPEs over different basin scales. Evaluation is carried out over the Upper Adige river basin (Eastern Italian Alps) for an eight-year period (2003-2010). Basin-average rainfall derived from a dense rain gauge network in the region is used as reference. Satellite precipitation error analysis is performed for warm (May to August) and cold (September to December) seaso...},
