keywords = {Arid region,GIS,HEC-HMS,IHACRES,SCS-CN method,Wadi Dhuliel},
number = {7},
pages = {2391--2409},
title = {{Modelling Rainfall Runoff Relations Using HEC-HMS and IHACRES for a Single Rain Event in an Arid Region of Jordan}},
volume = {27},
year = {2013}
}
