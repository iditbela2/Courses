doi = {10.1109/JSEN.2016.2514378},
file = {:Users/iditbela/Downloads/07370876.pdf:pdf},
isbn = {1530-437X VO  - 16},
issn = {1530-437X},
journal = {IEEE Sensors Journal},
keywords = {Air pollution,Air quality monitoring,Atmospheric modeling,Data models,Forecasting,M5P model trees,Market research,Monitoring,NO{\textless}sub{\textgreater}2{\textless}/sub{\textgreater},O{\textless}sub{\textgreater}3{\textless}/sub{\textgreater},Predictive models,SO{\textless}sub{\textgreater}2{\textless}/sub{\textgreater},Web portal,air pollution forecasting models,air pollution measurement,air quality,artificial neural networks,environmental monitoring (geophysics),environmental science computing,forecasting,gas sensors,gaseous sensor,geophysics computing,ground level ozone concentration,historical information,intelligent sensing platform,low cost air quality monitoring motes,machine learning algorithms,meteorological instruments,meteorological sensor,mobile application,multivariate forecasting model,neural nets,nitrogen dioxide concentration,short message service,sulfur dioxide concentration,support vector machines,univariate forecasting model,urban air pollution monitoring system,wireless sensor networks,wireless sensors network},
