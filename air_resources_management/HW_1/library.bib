pages = {2172--2182},
title = {{The Supercooled Warm Rain Process and the Specification of Freezing Precipitation}},
url = {http://journals.ametsoc.org/doi/abs/10.1175/1520-0493{\%}281988{\%}29116{\%}3C2172{\%}3ATSWRPA{\%}3E2.0.CO{\%}3B2{\%}5Cnhttp://journals.ametsoc.org/doi/pdf/10.1175/1520-0493{\%}281988{\%}29116{\%}3C2172{\%}3ATSWRPA{\%}3E2.0.CO{\%}3B2},
