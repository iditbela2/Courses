volume = {14},
year = {2011}
}
@article{Vivoni2006,
abstract = {The predictability of hydrometeorological flood events is investigated$\backslash$nthrough the combined use of radar nowcasting and distributed hydrologic$\backslash$nmodeling. Nowcasting of radar-derived rainfall fields can extend the$\backslash$nlead time for issuing flood and flash flood forecasts based on a$\backslash$nphysically based hydrologic model that explicitly accounts for spatial$\backslash$nvariations in topography, surface characteristics, and meteorological$\backslash$nforcing. Through comparisons to discharge observations at multiple$\backslash$ngauges ( at the basin outlet and interior points), flood predictability$\backslash$nis assessed as a function of forecast lead time, catchment scale, and$\backslash$nrainfall spatial variability in a simulated real-time operation. The$\backslash$nforecast experiments are carried out at temporal and spatial scales$\backslash$nrelevant for operational hydrologic forecasting. Two modes for temporal$\backslash$ncoupling of the radar nowcasting and distributed hydrologic models ($\backslash$ninterpolation and extended-lead forecasting) are proposed and evaluated$\backslash$nfor flood events within a set of nested basins in Oklahoma. Comparisons$\backslash$nof the radar-based forecasts to persistence show the advantages of$\backslash$nutilizing radar nowcasting for predicting near-future rainfall during$\backslash$nflood event evolution.},
