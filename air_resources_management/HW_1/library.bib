@misc{Ciach1997,
abstract = {A simplified probability matching method is introduced that relies on$\backslash$nmatching the unconditional probabilities of R and Z(e) using data from a$\backslash$nC-band radar and raingage network near Darwin, Australia. This is$\backslash$nachieved by matching raingage intensities to radar reflectivities taken$\backslash$nonly from small `'windows'' centered about the gauges in time and space.$\backslash$nThe windows must be small enough for the gauge to represent the rainfall$\backslash$ndepth within the radar window yet large enough to encompass the timing$\backslash$nand geometrical errors inherent to such coincident observations. The$\backslash$ncalculation of the Z(e)-R relation with the window probability matching$\backslash$nmethod (WPMM) is quite straightforward, whereby the unconditional$\backslash$ncumulative probabilities of Z(e) and R, which are obtained from all of$\backslash$nthe windows, are matched. In practice Z(e) and R, having the same$\backslash$ncumulative percentile, are related to each other.$\backslash$nA relatively small sample size (about 600 mm for all gauges combined) is$\backslash$nrequired to achieve a stable Z(e)-R relation with a standard deviation$\backslash$nof 15{\%} of R for a given Z(e). The obtained Z(e)-R relations are curved$\backslash$nlines in log-log space and therefore may better represent the$\backslash$ntransformation of Z(e) into R than any straight-line power law.$\backslash$nThe WPMM also performs significantly better for rainfall integrations$\backslash$nthan power law. The standard deviation of the WPMM rainfall integration,$\backslash$nafter correction for systematic bias errors, is only two-thirds that of$\backslash$nthe standard deviation obtained when using a power law based on$\backslash$ndisdrometer measured drop size distribution.$\backslash$nAdditional improvement in the accuracy of the WPMM is provided upon its$\backslash$napplication to data that has been objectively classified into different$\backslash$nrain regimes, which is the topic of another related study in this$\backslash$njournal.},
author = {Ciach, Grzegorz J. and Krajewski, Witold F. and Smith, James a.},
booktitle = {Journal of Applied Meteorology},
doi = {10.1175/1520-0450(1997)036<0243:COTWPM>2.0.CO;2},
file = {:Users/iditbela/Library/Application Support/Mendeley Desktop/Downloaded/Ciach, Krajewski, Smith - 1997 - Comments on “The Window Probability Matching Method for Rainfall Measurements with Radar”.pdf:pdf},
issn = {0894-8763},
