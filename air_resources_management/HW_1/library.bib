@article{Fallis2013,
abstract = {Predicting the binding mode of flexible polypeptides to proteins is an important task that falls outside the domain of applicability of most small molecule and protein−protein docking tools. Here, we test the small molecule flexible ligand docking program Glide on a set of 19 non-$\alpha$-helical peptides and systematically improve pose prediction accuracy by enhancing Glide sampling for flexible polypeptides. In addition, scoring of the poses was improved by post-processing with physics-based implicit solvent MM- GBSA calculations. Using the best RMSD among the top 10 scoring poses as a metric, the success rate (RMSD ≤ 2.0 {\AA} for the interface backbone atoms) increased from 21{\%} with default Glide SP settings to 58{\%} with the enhanced peptide sampling and scoring protocol in the case of redocking to the native protein structure. This approaches the accuracy of the recently developed Rosetta FlexPepDock method (63{\%} success for these 19 peptides) while being over 100 times faster. Cross-docking was performed for a subset of cases where an unbound receptor structure was available, and in that case, 40{\%} of peptides were docked successfully. We analyze the results and find that the optimized polypeptide protocol is most accurate for extended peptides of limited size and number of formal charges, defining a domain of applicability for this approach.},
archivePrefix = {arXiv},
arxivId = {arXiv:1011.1669v3},
author = {Fallis, A.G},
doi = {10.1017/CBO9781107415324.004},
eprint = {arXiv:1011.1669v3},
file = {:Users/iditbela/Library/Application Support/Mendeley Desktop/Downloaded/Fallis - 2013 - No Title No Title.pdf:pdf},
isbn = {9788578110796},
issn = {1098-6596},
journal = {Journal of Chemical Information and Modeling},
keywords = {icle},
number = {9},
pages = {1689--1699},
pmid = {25246403},
title = {{No Title No Title}},
volume = {53},
year = {2013}
}
@article{DeVogel2004,
abstract = {Lake Eyre is the terminal playa in a large closed basin which receives the majority of its moisture from the summer monsoon. Modern Lake Eyre receives intermittent floodings at times of increased monsoon intensity, generally associated with the La Ni{\~{n}}a phase of the ENSO cycle. Lake Eyre was a perennial lake during several phases of increased monsoon activity during the late Quaternary, specifically about 125 ka, 80 ka, 65 ka, and 40 ka [Magee (1997) Late Quaternary Environments and Palaeohydrology of Lake Eyre, Arid Central Australia. Unpubl. Ph.D. Thesis, Australian National University, Canberra]. These lake highstands are reflected by elevated shorelines mapped around Lake Eyre and, to a lesser degree, around the Frome–Gregory system to the southeast of Lake Eyre. Comprehensive research in the Lake Eyre catchment over the last several decades has constrained the ages and elevations of these shoreline deposits, which now enable us to use digital elevation models to map these paleo-lakes. By incrementally filling each lake meter by meter, the areas and volumes of the paleo-lakes are calculated and the paleohydrologic processes, including the flow direction across spillovers, are reconstructed. At the peak 125-ka highstand, Lake Eyre stood at +10 m Australian Height Datum (AHD) and covered nearly 35 000 km2, more than three times the area of the current playa. Together with the Frome–Gregory system, it held 430 km3 of water. By contrast, the deepest historic filling held 30 km3. These lake levels represent climatic conditions very different from the modern situation. Due to discontinuous preservation of the shoreline deposits around the lakes and due to the scale of the lake systems, mapping the former levels of Lake Eyre and the Frome–Gregory system has been impossible prior to the availability of a digital elevation model. Paramatizing the area/volume relationships of water required to fill the lake to varying levels will provide data for quantification of climate and precipitation changes over time.},
author = {DeVogel, Stephen B. and Magee, John W. and Manley, William F. and Miller, Gifford H.},
doi = {10.1016/S0031-0182(03)00690-4},
file = {:Users/iditbela/Library/Application Support/Mendeley Desktop/Downloaded/DeVogel et al. - 2004 - A GIS-based reconstruction of late Quaternary paleohydrology Lake Eyre, arid central Australia.pdf:pdf},
issn = {00310182},
journal = {Palaeogeography, Palaeoclimatology, Palaeoecology},
keywords = {Digital Elevation Model,Geographic Information System,Lake Eyre,Lake Frome,Lake Gregory,lake levels,monsoon,paleo-shorelines},
month = {feb},
number = {1-2},
pages = {1--13},
title = {{A GIS-based reconstruction of late Quaternary paleohydrology: Lake Eyre, arid central Australia}},
volume = {204},
year = {2004}
}
@article{Reid1998,
abstract = {Comparatively little is known about the hydrology of desert flash-floods$\backslash$ndespite the extent of the world's drylands. There is even less known$\backslash$nabout their sedimentary behaviour and particularly about the movement$\backslash$nof coarse material as bedload. The results of an intense field monitoring$\backslash$nprogramme carried out on an ephemeral gravel-bed stream in the northern$\backslash$nNegev Desert are presented. In this semi-arid setting, flow duration$\backslash$nanalysis indicates that the channel is hydrologically active for$\backslash$n2{\%} of the time, or about seven days per year, and that overbank flow$\backslash$ncan be expected for only 0.03{\%} of the time or about three hours per$\backslash$nyear. Multipeaked flood hydrographs are the norm, reflecting many$\backslash$nfactors including the arrival of separate slugs of discharge from$\backslash$ncontributing subcatchments. The passage of the initial flood bore$\backslash$nis surprisingly slow, but the rising limb of the flood hydrograph$\backslash$nis rapid with a median time of rise of 10 minutes, in keeping with$\backslash$nexpected flash-flood behaviour. Bedload flux is high, averaging 2.67$\backslash$nkg s-1 m-1 during the period that the channel carries flow. This$\backslash$ngives very high bedload sediment yield despite the infrequent and$\backslash$nshort duration of flood flows and matches the high yield of suspended$\backslash$nsediment. The relationship between bedload flux and boundary shear$\backslash$nstress is simple, in contrast with perennial gravel-bed streams,$\backslash$nand the exponent of the log-llog relationship is 1.52. Of great value$\backslash$nis that the behaviour of the Nahal Eshtemoa corroborates a pattern$\backslash$nestablished by the authors previously in a smaller tributary stream.},
author = {Reid, Ian and Laronne, Jonathan B and Powell, D Mark},
doi = {10.1002/(sici)1099-1085(19980330)12:4<543::aid-hyp593>3.0.co;2-c},
file = {:Users/iditbela/Library/Application Support/Mendeley Desktop/Downloaded/Reid, Laronne, Powell - 1998 - Flash-flood and bedload dynamics of desert gravel-bed streams.co:co},
isbn = {0885-6087},
issn = {0885-6087},
journal = {Hydrol. Process.},
keywords = {arid-zone,ash-,bedload sediment,gravel-bed stream,ood,sediment yield},
number = {February 1997},
pages = {543--557},
title = {{Flash-flood and bedload dynamics of desert gravel-bed streams}},
volume = {12},
year = {1998}
}
