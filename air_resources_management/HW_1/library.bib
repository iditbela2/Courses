keywords = {Arid areas,Flood frequency,Flood volume,Israel,Partial duration series,Peak discharges,Regional model},
month = {jun},
number = {3-4},
pages = {204--219},
title = {{Frequency and magnitude of runoff events in the and Negev of Israel}},
