@article{Dayan2012,
abstract = {One of the most important goals of synoptic climatology is to analyse$\backslash$nthe relationships between atmospheric circulation and surface environmental$\backslash$nconditions. Since manual (subjective) classification might reflect$\backslash$ndifferently environmental phenomena as compared to computer assisted$\backslash$n(semi-objective) classification, a comparison between both classifications$\backslash$nwas performed for the Eastern Mediterranean (EM) for 10 years. The$\backslash$noverall frequencies of the 19 recurring synoptic types characterising$\backslash$nthe circulation regime over the EM are similar for both methods (correlation$\backslash$ncoefficient = 0.96, p-value {\textless} 0.0001). Nevertheless, comparing the$\backslash$nclassifications on a day-to-day basis for the three most common types$\backslash$nshowed a large degree of disagreement between the two. For synoptic$\backslash$ntypes with a deep horizontal pressure gradient both classifications$\backslash$nyielded the highest agreement (over 50{\%}), improving the consistency$\backslash$nbetween both classifications.$\backslash$n$\backslash$n$\backslash$nYarnal's (1993) ‘environment-to-circulation' approach was tested on$\backslash$nthree surface environment processes: air pollutants, desert dust$\backslash$nintrusions, and flash floods. Our results indicate that the weak$\backslash$npressure gradients associated with high ozone levels make the classification$\backslash$nmore difficult for both methods. Regarding dust outbreaks, classifications$\backslash$npoint on the importance of the cold cyclone location rather than$\backslash$nits pressure gradient. As for flash floods, the flow pattern at the$\backslash$nsurface level is insufficient to predict atmospheric conditions prone$\backslash$nfor their occurrence, suggesting that upper air data is an essential$\backslash$nfactor for determining such highly convective events.$\backslash$n$\backslash$n$\backslash$nFinally, two new approaches to evaluate the quality of classifications$\backslash$nare presented, demonstrated on two frequent circulation systems persisting$\backslash$nover the EM. A qualitative approach compares the composite mean sea$\backslash$nlevel pressure maps from each classification, while a quantitative$\backslash$napproach compares the resultant winds at a central site.$\backslash$n$\backslash$n$\backslash$nThis study demonstrates that comparing different synoptic classification$\backslash$nmethodologies and different environmental applications can lead to$\backslash$nadditional and valuable insights on the interaction between the environment$\backslash$nand synoptic-scale circulation.},
author = {Dayan, Uri and Tubi, Amit and Levy, Ilan},
doi = {10.1002/joc.2297},
file = {:Users/iditbela/Library/Application Support/Mendeley Desktop/Downloaded/Dayan, Tubi, Levy - 2012 - On the importance of synoptic classification methods with respect to environmental phenomena.pdf:pdf},
isbn = {1097-0088},
issn = {08998418},
journal = {International Journal of Climatology},
keywords = {Circulation type classification,Computer assisted classification,Dust outbreaks,Eastern Mediterranean,Flash floods,Manual classification,Synoptic circulation,Tropospheric ozone},
number = {5},
pages = {681--694},
title = {{On the importance of synoptic classification methods with respect to environmental phenomena}},
volume = {32},
