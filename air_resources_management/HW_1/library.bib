journal = {Water Quality, Exposure and Health},
keywords = {Artificial intelligence methods,Melen river,Pollution source,Water-quality parameters,artificial intelligence methods {\'{a}},pollution,source {\'{a}} water-quality parameters,{\'{a}} melen river},
